{
  "name": "kraut-ipsum-filler",
  "displayName": "Kraut Ipsum Filler Text",
  "description": "Fetches some traditional German filler text from the API at www.krautipsum.com and inserts it at the current position in the current document.",
  "version": "0.0.1",
  "publisher": "maec",
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.insertKraut"
  ],
  "main": "./out/src/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertKraut",
        "title": "Kraut einf√ºgen"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install"
  },
  "devDependencies": {
    "typescript": "^2.0.3",
    "vscode": "^1.0.0",
    "mocha": "^2.3.3",
    "@types/node": "^6.0.40",
    "@types/mocha": "^2.2.32"
  },
  "dependencies": {
    "http": "0.0.0"
  }
}
